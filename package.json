{
  "name": "my-starter",
  "version": "1.0.0",
  "description": "A starter kit for those who are tired of CRA, and do not like Gatsby or Next",
  "main": "index.js",
  "scripts": {
    "serve": "NODE_ENV=serve webpack-dev-server --open --colors --config webpack.config.serve.js",
    "serve:win": "cross-env NODE_ENV=serve webpack-dev-server --open --colors --config webpack.config.serve.js",
    "test": "jest --coverage --watchAll --config=jest.config.js",
    "lint": "cross-env NODE_ENV=serve eslint . --ext .ts",
    "build": "rm -rf ./dist && NODE_ENV=prod webpack --config webpack.config.prod.js",
    "build:win": "rmdir /S /Q dist && cross-env NODE_ENV=prod webpack --config webpack.config.prod.js"
  },
  "keywords": [],
  "author": "jiazhenghao <jiazhenghao0203@hotmail.com> (https://github.com/jiazhenghao)",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/react": "11.1.0",
    "@types/jest": "26.0.15",
    "@types/react": "16.9.53",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.6",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "compression-webpack-plugin": "6.0.4",
    "copy-webpack-plugin": "6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "5.0.0",
    "dotenv-webpack": "4.0.0",
    "eslint": "7.12.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.4.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.1.1",
    "fork-ts-checker-webpack-plugin": "4.1.3",
    "html-webpack-plugin": "4.5.0",
    "jest": "26.6.1",
    "prettier": "2.1.2",
    "react-test-renderer": "17.0.1",
    "source-map-loader": "1.1.1",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "3.0.6",
    "ts-jest": "26.4.3",
    "ts-loader": "8.0.7",
    "typescript": "4.0.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.0"
  }
}
